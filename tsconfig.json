{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "removeComments": false,
		"types": ["reflect-metadata"],
        "emitDecoratorMetadata": true,
        "noImplicitAny": true,
        "noImplicitThis": true

    },
    "exclude": [
      "node_modules"
    ],
	"include": [
		"src/*"
	]
}